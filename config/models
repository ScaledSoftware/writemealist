User
    ident Text
    UniqueUser ident
    deriving Typeable

List
    name Text
    createdBy UserId

Category
    name Text
    precedence Int
    listId ListId

ListEditor 
    list ListId
    viewer UserId
    UniqueListEditor list viewer

ListViewer
    listId         ListId  -- list that is shared
    sharingUserId  UserId  -- user that shared the list
    randKey        Int     -- a random number for this url

ListItem
    name Text
    list ListId
    createdBy UserId
    createdAt UTCTime
    completedBy UserId Maybe
    completedAt UTCTime Maybe
    catId CategoryId Maybe


