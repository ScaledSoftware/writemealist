<div>
   <h1> #{listName list}
   <ul>
     $forall (Entity listEntryId listEntry) <- listItems
       <li> 
          #{listEntryItem listEntry} 
          <form method="get" action="@{ListEntryEditR listId listEntryId}" style=" display:inline!important;">
            <input type="submit" value="_{MsgEditItem}">
          <form method="post" action="@{ListEntryDeleteR listId listEntryId}" style=" display:inline!important;">
            <input type="submit" value="_{MsgDeleteItem}">
