<h1 #siteWelcomeH1>
    _{MsgWelcome}
    <a href="@{HomeR}">_{MsgWriteMeAList}
<div #loginOutDiv>
  <p>
      $maybe _ <- mAuth
          <a href=@{AuthR LogoutR}>Logout
      $nothing
          <a href=@{AuthR LoginR}>Login
<div #listItemEditDiv>
    <h1> _{MsgEditEntryHeader} #{listName list}
    <form #editListItemForm method=post enctype=#{enctype} action="@{ListItemEditR listId listItemId}">
        ^{entrywidget}
        <div>
            <input .curve .gradient type=submit value=_{MsgEditListItemButton}>
